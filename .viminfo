	" Drawit插件设置  key: \dri
	
	"""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""
	
	function Drawit_f2()
	
	  if exists("g:loaded_drawit")
	
	        exe "DIstop"
	
	        unlet g:loaded_drawit
	
	  else
	
	      exe "DIstart"
	
	      let g:loaded_drawit = 0
	
	  endif
	
	endfunction
	
	noremap <leader>dri :call Drawit_f2()<CR>
	
	 
	
	 
	
